{
  "name": "prisonstaffhub",
  "version": "0.0.1",
  "private": false,
  "engines": {
    "npm": ">=3",
    "node": ">=10.*"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^0.0.17-alpha",
    "agentkeepalive": "^4.0.2",
    "applicationinsights": "^1.4.0",
    "applicationinsights-native-metrics": "^0.0.5",
    "axios": "0.18.1",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-middleware": "^0.8.0",
    "classnames": "^2.2.6",
    "connect-flash": "^0.1.1",
    "connect-redis": "^4.0.3",
    "cookie-session": "^2.0.0-beta.3",
    "csurf": "^1.11.0",
    "csv-parse": "^4.4.6",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-form-data": "^2.0.7",
    "express-session": "^1.16.2",
    "final-form": "^4.11.0",
    "govuk-colours": "^1.0.3",
    "govuk-elements-sass": "^3.1.2",
    "govuk-frontend": "^3.6.0",
    "govuk_frontend_toolkit": "^7.5.0",
    "helmet": "^3.12.1",
    "hsts": "^2.1.0",
    "isbinaryfile": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "moment-business-days": "^1.1.3",
    "nocache": "^2.1.0",
    "nock": "^11.9.0",
    "notifications-node-client": "^4.7.0",
    "nunjucks": "^3.2.0",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.4.0",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.7.2",
    "qs": "^6.5.2",
    "query-string": "^6.2.0",
    "redis": "^2.8.0",
    "sass": "^1.26.12",
    "shortid": "2.2.14",
    "spin.js": "^3.1.0",
    "superagent": "^5.1.0",
    "universal-analytics": "^0.4.20"
  },
  "devDependencies": {
    "audit-ci": "^3.0.0",
    "axios-mock-adapter": "^1.15.0",
    "chai": "^4.1.2",
    "cypress": "^5.0.0",
    "cypress-file-upload": "^4.0.7",
    "cypress-multi-reporters": "^1.4.0",
    "dotenv-expand": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^3.0.5",
    "jest": "^24.8.0",
    "jest-useragent-mock": "^0.0.3",
    "jsdom": "^13.0.0",
    "lint-staged": "^8.2.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0",
    "nodemon": "^2.0.4",
    "nyc": "15.1.0",
    "prettier": "1.14.3",
    "process": "0.11.10",
    "raf": "^3.4.0",
    "set-cookie-parser": "^2.2.1",
    "sinon": "^6.1.3",
    "sinon-chai": "^3.2.0",
    "style-loader": "^0.21.0",
    "supertest": "^3.1.0",
    "whatwg-fetch": "2.0.4"
  },
  "scripts": {
    "clean": "rm -rf .nyc_output coverage",
    "start": "node backend/index.js",
    "start:cov": "nyc node backend/index.js",
    "start:dev": "nodemon --watch backend --watch views backend/index.js",
    "start-feature": "npm run compile-sass && export $(cat $npm_config_env) && node backend/index.js",
    "build": "npm run compile-sass",
    "test": "jest",
    "test-watch": "jest --watch",
    "record-build-info": "node scripts/record-build-info",
    "lint": "node node_modules/eslint/bin/eslint.js src backend",
    "verify": "npm run lint && npm test",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "int-test": "cypress run",
    "int-test-no-artefacts": "cypress run --config video=false,screenshotOnRunFailure=false",
    "int-test-ui": "cypress open",
    "compile-sass": "sass --load-path=. ./sass:./build/static/stylesheets --style compressed",
    "compile-sass-watch": "npm run compile-sass && sass --load-path=. ./sass:./build/static/stylesheets --watch --style compressed"
  },
  "nyc": {
    "all": true,
    "include": [
      "backend/**/*.js"
    ],
    "reporter": [
      "html"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "backend/**/*.{js,mjs}"
    ],
    "globalSetup": "<rootDir>/config/jest/globalSetup.js",
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/integration-tests/"
    ],
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!(new-nomis-shared-components|spin.js)/).+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "node",
      "mjs"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "package.json"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".circleci/*",
      "migrations/*",
      "seeds/*",
      "node_modules/*",
      "integration-tests/*"
    ],
    "delay": 2500,
    "ext": "js,njk"
  }
}
